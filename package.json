{
  "name": "tailwind-nextjs-starter-blog",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "cross-env INIT_CWD=$PWD next dev",
    "build": "cross-env INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/postbuild.mjs",
    "build:static": "cross-env INIT_CWD=$PWD NEXT_EXPORT=true next build",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir app --dir components --dir lib --dir layouts --dir scripts",
    "lint:check": "next lint --dir app --dir components --dir layouts --dir scripts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore ."
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.2.18",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@vercel/analytics": "^1.4.1",
    "autoprefixer": "^10.4.20",
    "classix": "^2.2.6",
    "contentlayer2": "^0.5.4",
    "esbuild": "^0.24.0",
    "feed": "^5.1.0",
    "flowbite": "^2.5.2",
    "flowbite-react": "^0.10.2",
    "framer-motion": "^12.23.25",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "image-size": "1.1.1",
    "lucide-react": "^0.460.0",
    "next": "14.2.18",
    "next-contentlayer2": "^0.5.4",
    "next-themes": "^0.4.3",
    "pliny": "0.3.1",
    "postcss": "^8.4.49",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-toastify": "^10.0.6",
    "react-wrap-balancer": "^1.1.1",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-citation": "^2.2.2",
    "rehype-katex": "^7.0.1",
    "rehype-preset-minify": "7.0.1",
    "rehype-prism-plus": "^2.0.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.15",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/mdx": "^2.0.13",
    "@types/react": "^18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.18",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "typescript": "^5.7.2"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.6.1"
}
